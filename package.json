{
  "name": "@ngageoint/geopackage-kml-js",
  "version": "4.1.0",
  "description": "KML to GeoPackage converter",
  "keywords": [
    "NGA",
    "KML",
    "KMZ",
    "mapcache"
  ],
  "license": "MIT",
  "author": {
    "name": "NGA",
    "url": "https://www.nga.mil"
  },
  "contributors": [
    "Daniel Barela <daniel.barela@gmail.com>",
    "Jared Lincenberg <jaredlincenberg@gmail.com>"
  ],
  "homepage": "http://ngageoint.github.io/geopackage-kml-js/",
  "repository": {
    "type": "git",
    "url": "https://github.com/ngageoint/geopackage-kml-js.git"
  },
  "bugs": {
    "url": "https://github.com/ngageoint/geopackage-kml-js/issues"
  },
  "bin": "./cli",
  "dependencies": {
    "@ngageoint/geopackage": "4.1.0",
    "@turf/bbox": "6.0.1",
    "@turf/turf": "5.1.6",
    "axios": "0.19.2",
    "browser-or-node": "1.3.0",
    "fs": "0.0.1-security",
    "jimp": "0.16.0",
    "jszip": "3.5.0",
    "lodash": "4.17.15",
    "xml-stream-saxjs": "0.4.6"
  },
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/browser-or-node": "^1.2.0",
    "@types/lodash": "^4.14.155",
    "@types/mkdirp": "^1.0.1",
    "chai": "4.2.0",
    "coveralls": "^3.0.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint": "^7.6.0",
    "gyp": "^0.5.0",
    "mocha-lcov-reporter": "^1.3.0",
    "mocha": "^6.2.3",
    "nyc": "^15.1.0",
    "prettier": "^2.0.5",
    "source-map-support": "^0.5.19",
    "ts-loader": "^8.0.2",
    "ts-node": "^8.5.2",
    "typescript": "^3.9.6",
    "typedoc": "^0.15.4"
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "temp-dir": "./.test_run",
    "report-dir": "./docs/coverage",
    "reporter": [
      "lcov"
    ]
  },
  "main": "built/index.js",
  "types": "built/index.d.ts",
  "files": [
    "built",
    "index.ts",
    "cli",
    "lib",
    "geoSpatialUtilities.ts",
    "imageUtilities.ts",
    "kmlUtilities.ts"
  ],
  "scripts": {
    "gh-pages-build": "npm install && npm run typedoc --options typedoc.json",
    "typedoc": "rm -rf ./api-docs; typedoc --tsconfig tsconfig.json --mode file --out docs/api index.ts",
    "test": "npm run clean-built && nyc mocha -r ts-node/register",
    "report": "nyc report",
    "tsc": "rm -rf ./built && tsc; cp ./node_modules/@ngageoint/geopackage/dist/canvaskit/canvaskit.wasm built/canvaskit.wasm",
    "prepublishOnly": "rm -rf ./built && npm run tsc",
    "build": "npm run clean && webpack && tsc",
    "clean-built": "rm -rf ./built",
    "clean": "rm -rf ./.test_run; rm -rf ./.nyc_output; rm -rf ./docs/coverage; rm -rf docs/api"
  }
}
